<h1>Showing all destinations</h1>
<table>
  <thead>
    <tr>
      <th>Country</th>
      <th>City</th>
      <th>Description</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @destination.each do |destination| %>
    <tr>
      <td><%= destination.country %></td>
      <td><%= destination.city %></td>
      <td><%= destination.description %></td>
      <td>View,Edit,Delete</td>
    </tr>
    <% end %>
  </tbody>
</table>
